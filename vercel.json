{
  "buildCommand": "git lfs pull && npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "routes": [
    {
      "src": "/models/.*",
      "dest": "/$0"
    },
    {
      "src": "/drawings/.*",
      "dest": "/$0"
    },
    {
      "src": "/assets/.*",
      "dest": "/$0"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
